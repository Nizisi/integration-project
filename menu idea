def one():
  temp = input("Which year is qing dynasty：")
  guess = int(temp)
  if guess == 1636:
      print("right")
      print("nice job")
  else:
      print("wrong")
  print("study harder")
Question = input(" pick one question : ")
Qnum= int(Question)
if Qnum== 1:
  one()


